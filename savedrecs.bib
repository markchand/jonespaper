@article{ ISI:000255926700004,
Author = {Silander, Daniel and Klasson, Torgny},
Title = {{Images of threats in the Swedish security debate}},
Journal = {{INTERNASJONAL POLITIKK}},
Year = {{2008}},
Volume = {{66}},
Number = {{1}},
Pages = {{65+}},
Month = {{APR}},
Abstract = {{This article explores the Swedish security debate of 2006 and onwards.
   it is argued that the Swedish security debate has been politically
   divided; one fraction stressing an intensified threat image in
   transnational criminality towards the Swedish society and the other
   fraction questioning such an image. Previous research on security,
   securitization and speech act has stressed a widened definition of what
   security is. Scholars on securitization have argued that security may be
   socially constructed by policy entrepreneurs in a society. By exploring
   the Swedish security debate, this article identifies a process of
   securitization and a highly contested Swedish security analysis.}},
