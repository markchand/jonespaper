@article{ ISI:000260253400002,
Author = {Menahem, Gila},
Title = {{The Transformation of Higher Education in Israel since the 1990s: The
   Role of Ideas and Policy Paradigms}},
Journal = {{GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY ADMINISTRATION AND
   INSTITUTIONS}},
Year = {{2008}},
Volume = {{21}},
Number = {{4}},
Pages = {{499-526}},
Month = {{OCT}},
Abstract = {{This article examines the transformation of Israel's higher education
   system since the 1990s. During that period, the system underwent
   expansion, diversification, privatization, and internationalization in a
   series of path-breaking reforms. The main argument is that while
   external factors-such as demograpic trends-exerted pressure for change,
   the trajectory and policy options preferred were shaped by ideational
   factors. Policy entrepreneurs played a crucial role in advancing
   pathbreaking institutional change when they reframed policies through
   linking cognitive ideas of ``what has to be done{''} with the normative
   ideas that granted legitimacy to the proposals for reform.}},
