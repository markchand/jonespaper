@article{ ISI:A1995TN43100006,
Author = {Smith, JD and Glick, HR},
Title = {{The right to die: A cross-national analysis of agenda setting and
   innovation}},
Journal = {{ENVIRONMENT AND PLANNING C-GOVERNMENT AND POLICY}},
Year = {{1995}},
Volume = {{13}},
Number = {{4}},
Pages = {{479-501}},
Month = {{NOV}},
Abstract = {{Through theories of agenda setting and innovation, the origin,
   development, and enactment of right-to-die policy in four Western
   nations-the United States, the Netherlands, Germany, and Great
   Britain-are examined. Different social and government structures
   produced varied right-to-die politics in each of these countries,
   although similar issues received more emphasis in Europe. However, it is
   discovered that policy entrepreneurs, organizations, and governments are
   important in similar ways in moving the issue from the public to the
   governmental agenda and to policy innovations in each country. The paper
   is concluded with a discussion of elements to be included in a model of
   agenda setting and innovation and with a proposal for the application of
   theory to a wider range of policies.}},
