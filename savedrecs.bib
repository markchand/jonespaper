@article{ ISI:A1992HK83400012,
Author = {HOLBROOK, TM and PERCY, SL},
Title = {{EXPLORING VARIATIONS IN STATE LAWS PROVIDING PROTECTIONS FOR PERSONS
   WITH DISABILITIES}},
Journal = {{WESTERN POLITICAL QUARTERLY}},
Year = {{1992}},
Volume = {{45}},
Number = {{1}},
Pages = {{201-220}},
Month = {{MAR}},
Abstract = {{Long before the Americans With Disabilities Act at the national level,
   the states were very active in the area of disability rights
   legislation.  As with many other policy areas, the degree of commitment
   to disability rights-as reflected in the stringency of disability rights
   legislation-varies substantially across states.  Somewhat surprisingly,
   students of comparative state politics have neglected this policy area. 
   In this paper a model of the determinants of state disability rights
   policy is developed and tested.  The results of the analysis indicate
   that competitive Democratic states and states with a history of what we
   call positive government policies are likely to have the most stringent
   laws.  It is also suggested that some variation in state laws may also
   be due to other, yet to be tested, influences, such as the relative
   strength of activist groups and the presence policy entrepreneurs.}},
