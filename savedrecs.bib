@article{ ISI:A1993KX63200002,
Author = {WILDER, RJ},
Title = {{COOPERATIVE GOVERNANCE, ENVIRONMENTAL-POLICY, AND MANAGEMENT OF OFFSHORE
   OIL AND GAS IN THE UNITED-STATES}},
Journal = {{OCEAN DEVELOPMENT AND INTERNATIONAL LAW}},
Year = {{1993}},
Volume = {{24}},
Number = {{1}},
Pages = {{41-62}},
Month = {{JAN-MAR}},
Abstract = {{The domestic American regime for management of offshore oil and gas is
   based on strict division of state from federal authority exactly 3 miles
   seaward. However, this approach has been characterized by stalemate, as
   the federal budget process is used by state representatives in Congress
   to halt further development. Given recent U. S. extension of its
   territorial sea from 3 to 12 miles, the 3-mile state-federal division
   may now be replaced with a more progressive regime based on cooperative
   state/federal governance. Adopting a framework offered by John Kingdom
   in Agendas, Alternatives and Public Policies, specific conditions are
   identified under which cooperative governance will replace the present
   scheme. In applying Kingdom's model, change in the presidency from Bush
   to Clinton is seen as the window of opportunity for achieving this new
