@article{ ISI:000323218300002,
Author = {Arieli, Tamar and Cohen, Nissim},
Title = {{Policy entrepreneurs and post-conflict cross-border cooperation: a
   conceptual framework and the Israeli-Jordanian case}},
Journal = {{POLICY SCIENCES}},
Year = {{2013}},
Volume = {{46}},
Number = {{3}},
Pages = {{237-256}},
Month = {{SEP}},
Abstract = {{What is the role of policy entrepreneurs in shaping the dynamics which
   normalize relationships between neighboring countries after the formal
   resolution of conflict? This article suggests a conceptual framework to
   understand the influence of policy entrepreneurs on public policy
   regarding cross-border interaction in post-conflict border regions. We
   analyze the motivations, preferences and strategies of local players
   which design given realities in the border region. We propose a typology
   of the various policy entrepreneurs active in the post-conflict border
   region. The theoretical framework is used in the analysis of the
   Israeli-Jordanian border region since the peace treaty of 1994.}},
