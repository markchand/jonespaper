@article{ ISI:A1997YJ82300001,
Author = {Aberbach, JD and Rockman, BA},
Title = {{Back to the future? Senior federal executives in the United States}},
Journal = {{GOVERNANCE-AN INTERNATIONAL JOURNAL OF POLICY AND ADMINISTRATION}},
Year = {{1997}},
Volume = {{10}},
Number = {{4}},
Pages = {{323-349}},
Month = {{OCT}},
Abstract = {{Based on findings from the 1970s, research literature on senior
   government executives emphasized a growing integration of politics and
   administration. This integration was reflected in what we called the
   `'Image IV'' bureaucrat wherein political role traits combined with
   those of traditional bureaucratic ones. Although this was by no means a
   dominant trend, we were led to speculate that traditional divisions
   between political and bureaucratic roles were eroding. Data gathered
   from the 1980s and 1990s, however, lead us to infer that divisions
   between political and bureaucratic roles have reasserted themselves and
   that integration between them is being diminished rather than
   strengthened. We conclude that this may be because in an era of
   governmental austerity the demand for executive policy entrepreneurs has
   slackened while the political needs have shifted to those of managerial
   control over an inertial or even contracting state.}},
