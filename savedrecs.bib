@article{ ISI:000382590200004,
Author = {Zhu, Xufeng and Zhang, Peipei},
Title = {{Intrinsic Motivation and Expert Behavior: Roles of Individual Experts in
   Wenling Participatory Budgeting Reform in China}},
Journal = {{ADMINISTRATION \& SOCIETY}},
Year = {{2016}},
Volume = {{48}},
Number = {{7}},
Pages = {{851-882}},
Month = {{SEP}},
Abstract = {{Previous theories on expert involvement have been classically based on
   the assumption of extrinsic influence-driven motivation. This article
   aims to construct an expert behavior theory to bridge the gap between
   intrinsic motivation and behavior of experts by identifying two
   dimensions, namely, the attitudes toward two relationships, between
   theory and practice and between experts and officials. These two
   intrinsic motivations determine the four roles of individual experts,
   namely, technology communicators, theory demonstrators, idea
   entrepreneurs, and knowledge brokers. The empirical strategy is based on
   a comparative study of four experts who were involved in the Wenling
   participatory budgeting reform in China.}},
