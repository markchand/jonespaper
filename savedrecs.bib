@article{ ISI:000333033900001,
Author = {Heikkila, Tanya and Pierce, Jonathan J. and Gallaher, Samuel and Kagan,
   Jennifer and Crow, Deserai A. and Weible, Christopher M.},
Title = {{Understanding a Period of Policy Change: The Case of Hydraulic
   Fracturing Disclosure Policy in Colorado}},
Journal = {{REVIEW OF POLICY RESEARCH}},
Year = {{2014}},
Volume = {{31}},
Number = {{2}},
Pages = {{65-87}},
Month = {{MAR}},
Abstract = {{This paper investigates the beliefs and framing strategies of interest
   groups during a period of policy change and the factors explaining
   policy change. We develop propositions to explore questions concerning
   policy change primarily from the advocacy coalition framework as well as
   from other theorie. The propositions are tested by examining the
   promulgation of a Colorado regulation requiring the disclosure of
   chemicals used in hydraulic fracturing. Using coded data of documents
   published by organizations involved in the rulemaking process, we find
   divergence between industry and environmental groups on their beliefs
   concerning hydraulic fracturing, as well as their portraying themselves
   and each other as heroes, victims, and villains, but some convergence on
   their more specific beliefs concerning disclosure of chemicals.
   Interviews point to the importance of policy entrepreneurs, timing, a
   negotiated agreement, and learning for explaining policy change. The
   findings provide both theoretical and methodological insights into how
   and why policy changes.}},
