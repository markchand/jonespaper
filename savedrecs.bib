@article{ ISI:000261656000003,
Author = {Zhu, Xufeng},
Title = {{Strategy of Chinese policy entrepreneurs in the third sector: challenges
   of ``Technical Infeasibility{''}}},
Journal = {{POLICY SCIENCES}},
Year = {{2008}},
Volume = {{41}},
