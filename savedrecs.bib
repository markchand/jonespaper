@article{ ISI:000342528300029,
Author = {Sutton, Abigail M. and Rudd, Murray A.},
Title = {{Deciphering contextual influences on local leadership in community-based
   fisheries management}},
Journal = {{MARINE POLICY}},
Year = {{2014}},
Volume = {{50}},
Number = {{A}},
Pages = {{261-269}},
Month = {{DEC}},
Abstract = {{Community-based fisheries management (CBFM) strategies have been adopted
   in a variety of small-scale fisheries around the world. Within these
   management structures, leaders are increasingly regarded as essential
   for viable CBFM yet systematic analysis into the intricate mechanisms of
   leadership are limited. This paper aims to identify key knowledge gaps
   of leadership in CBFM by strategically reviewing research from fisheries
   and natural resource management, and from other sectors. The focus is on
   the interaction between leaders, their connections with and beyond their
   communities, and the context within which leaders function. Insights
   from over 30 case studies suggest previous work on leaders and
   leadership generally focused on relatively coarse-scale characteristics
   of leadership and the functions that leaders perform. Ecological and
   social context influence leaders' ability to help deliver successful
   CBFM. The personal and professional attributes of leaders themselves may
   be beneficial or inhibitory for CBFM depending on that context. It is
   therefore essential that future research builds on current insight in
   order to decipher the implications of contextual influences on local
   leadership and, by extension, the level of CBFM success. (C) 2014
   Elsevier Ltd. All rights reserved.}},
