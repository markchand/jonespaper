@article{ ISI:000301907300004,
Author = {Thompson, Debra and Wallner, Jennifer},
Title = {{A Focusing Tragedy: Public Policy and the Establishment of Afrocentric
   Education in Toronto}},
Journal = {{CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE}},
Year = {{2011}},
Volume = {{44}},
Number = {{4}},
Pages = {{807-828}},
Month = {{DEC}},
Abstract = {{In January 2008, the Toronto District School Board (TDSB) voted to
   establish Afrocentric education. Earlier proposals dating back to the
   early 1990s, however, garnered little response from the board. Why was
   the TDSB suddenly more amenable to the idea? We argue that the death of
   Jordan Manners acted as a focusing event that triggered concern
   regarding school safety and the challenges facing Black youth in
   Toronto, altering the policy climate and opening a window of opportunity
   for change. This analysis furthers conceptualizations of the connections
   between focusing events, policy entrepreneurs, the viability of
   alternative proposals, and the catalysts that facilitate policy change.}},
